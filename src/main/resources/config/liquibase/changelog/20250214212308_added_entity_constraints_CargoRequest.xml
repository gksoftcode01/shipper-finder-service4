<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity CargoRequest.
    -->
    <changeSet id="20250214212308-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="from_country_id"
                                 baseTableName="cargo_request"
                                 constraintName="fk_cargo_request__from_country_id"
                                 referencedColumnNames="id"
                                 referencedTableName="country"
                                 />

        <addForeignKeyConstraint baseColumnNames="to_country_id"
                                 baseTableName="cargo_request"
                                 constraintName="fk_cargo_request__to_country_id"
                                 referencedColumnNames="id"
                                 referencedTableName="country"
                                 />

        <addForeignKeyConstraint baseColumnNames="from_state_id"
                                 baseTableName="cargo_request"
                                 constraintName="fk_cargo_request__from_state_id"
                                 referencedColumnNames="id"
                                 referencedTableName="state_province"
                                 />

        <addForeignKeyConstraint baseColumnNames="to_state_id"
                                 baseTableName="cargo_request"
                                 constraintName="fk_cargo_request__to_state_id"
                                 referencedColumnNames="id"
                                 referencedTableName="state_province"
                                 />
    </changeSet>
</databaseChangeLog>
